<% content_for :slider do%>
    <h1 class="h1_first">PROJECTS</h1>
      <table>
        <tr>
                    <th>Name</th>
                    <th>User</th>                    
                    <th>Organization</th>
                    <th>Cause</th>
                    <th>Location</th>
                    <th></th>
        </tr>
<% @projects.each do |project| %>
        <tr>
                    <td><%= link_to project.name, project %></td>
                    <td><%= link_to project.user.profile.screen_name, project.user.profile %></td>                    
                    <td><%= project.organization %></td>
                    <td><%= project.cause %></td>
                    <td><%= project.location %></td>
        </tr>
      <% end %>
      </table>
      <a href="/projects/new">Create New Project</a>
<% end %>





